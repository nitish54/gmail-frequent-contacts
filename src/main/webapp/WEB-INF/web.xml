<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Demo Webapp Gmail Search</display-name>
  
	  <servlet>
	    <servlet-name>Controller</servlet-name>
	    <servlet-class>com.sinha.google.servlet.Controller</servlet-class>
	</servlet>
	<servlet-mapping>
	    <servlet-name>Controller</servlet-name>
	    <url-pattern>/Controller</url-pattern>
	</servlet-mapping>
	
	<servlet>
	    <servlet-name>Revoke</servlet-name>
	    <servlet-class>com.sinha.google.servlet.Revoke</servlet-class>
	</servlet>
	<servlet-mapping>
	    <servlet-name>Revoke</servlet-name>
	    <url-pattern>/Revoke</url-pattern>
	</servlet-mapping>
	
	<servlet>
	    <servlet-name>getMail</servlet-name>
	    <servlet-class>com.sinha.google.servlet.GetMail</servlet-class>
	</servlet>
	<servlet-mapping>
	    <servlet-name>getMail</servlet-name>
	    <url-pattern>/getMail</url-pattern>
	</servlet-mapping>
</web-app>
